<div class="container">
    <h1>
        <%=title %>
    </h1>
    <div class="row">
        <div class="column">
            <table class="table table-striped table-bordered table-hover">
                <thead class="table-dark">
                    <tr>
                        <th scope="col" class="text-center">#</th>
                        <th scope="col" class="text-center">Nickname</th>
                        <th scope="col">Survey Topic</th>
                        <th>Date Taken</th>
                        <th></th>
                        <th></th>
                    </tr>
                </thead>
                <tbody id="surveyList">
                    <% for(let count=0; count < answers.length; count++) { %>
                        <tr>
                            <td scope="row" class="text-center"></td>
                            <td>
                                <%= answers[count].name%>
                            </td>
                            <td>
                                <%= answers[count].topic %>
                            </td>
                            <td>
                                <%= answers[count].createdAt %>
                            </td>
                            <% if( username) { %>
                                </td>
                                <td class="text-center col-1">
                                    <a class="btn btn-warning" href="/results-delete/<%= answers[count].id %>">
                                        Delete
                                    </a>
                                </td>
                                <% } else { %>
                                    <td class="text-center col-1">
                                        <a class="btn btn-primary" href="/results-details/<%= answers[count].id %>">
                                            Details
                                        </a>
                                    </td>
                                    <% } %>
                        </tr>
                        <% } %>
                </tbody>
            </table>
        </div>
    </div>
</div>